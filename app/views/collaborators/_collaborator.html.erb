<% @wiki.collaborators.each do |collaborator| %>
	<p>
		<%= collaborator.user.try(:email) %>
		<% if @wiki.user == current_user %>
			<div>
	    		<%= link_to "Remove Collaborator", collaborators_destroy_path(collaborator), method: :delete, class: 'btn btn-success' %>
			</div>
		<% end %>
	</p>
<% end %>